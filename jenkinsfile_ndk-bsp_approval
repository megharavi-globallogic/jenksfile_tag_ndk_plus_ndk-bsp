pipeline {
	agent none
	
	parameters {
    		string(name: 'version_release', defaultValue: '', description: 'enter tag?')
  	}
	
	options {
        timeout(time: 7, unit: 'DAYS')
    	}
	
    stages {
    	stage("Set version name"){
        	steps {
                	script {
                    		currentBuild.displayName = "${params.version_release}"
                	}
            	}
        }
    }
	
}
