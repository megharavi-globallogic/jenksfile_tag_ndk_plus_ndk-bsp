pipeline {
	agent none
	
	parameters {
    		string(name: 'VERSION_RELEASE', defaultValue: 'hello; ls /', description: 'What should I say?')
  	}
	
	options {
        timeout(time: 7, unit: 'DAYS')
    	}
	
    stages {
    	stage("Set version name"){
        	steps {
                	script {
                    		currentBuild.displayName = "${params.VERSION_RELEASE}"
                	}
            	}
        }
    }
	
}
